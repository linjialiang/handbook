server
{
    listen 80;
    server_name vite.docs.e8so.com;
    root /server/www/docs/vite-docs-cn;
    access_log      off;
    log_not_found   off;
    index index.html;
    include custom/no_access;
    if ($request_method !~* GET|POST)
    {
        return 403;
    }
}
