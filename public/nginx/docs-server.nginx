server
{
    listen 80;
    server_name axios.docs.e8so.com;
    root /server/www/docs/axios-docs;
    access_log      off;
    log_not_found   off;
    index index.html;
    location ~ /(zh/)?docs/.+$
    {
        try_files $uri $uri.html;
    }
    include custom/no_access;
    if ($request_method !~* GET|POST)
    {
        return 403;
    }
}
